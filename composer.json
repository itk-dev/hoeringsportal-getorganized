{
    "name": "itk-dev/hoeringsportal-getorganized",
    "description": "Archiving documents in GetOrganized",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "deskpro/deskpro-api-client-php": "^2.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "easycorp/easyadmin-bundle": "^3.5",
        "itk-dev/database-bundle": "^1.3",
        "itk-dev/getorganized-api-client-php": "^1.0",
        "kapersoft/sharefile-api": "dev-deltag.aarhus.dk",
        "mpdf/mpdf": "^8.0",
        "opis/json-schema": "^2.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpoffice/phpspreadsheet": "^1.22",
        "phpstan/phpdoc-parser": "^1.4",
        "sensio/framework-extra-bundle": "^6.2",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "~5.4.0",
        "symfony/console": "~5.4.0",
        "symfony/dotenv": "~5.4.0",
        "symfony/expression-language": "~5.4.0",
        "symfony/filesystem": "~5.4.0",
        "symfony/flex": "^1.18",
        "symfony/form": "~5.4.0",
        "symfony/framework-bundle": "~5.4.0",
        "symfony/mailer": "~5.4.0",
        "symfony/messenger": "~5.4.0",
        "symfony/monolog-bundle": "^3.7",
        "symfony/options-resolver": "~5.4.0",
        "symfony/process": "~5.4.0",
        "symfony/property-access": "~5.4.0",
        "symfony/property-info": "~5.4.0",
        "symfony/proxy-manager-bridge": "~5.4.0",
        "symfony/runtime": "~5.4.0",
        "symfony/security-bundle": "~5.4.0",
        "symfony/serializer": "~5.4.0",
        "symfony/translation": "~5.4.0",
        "symfony/twig-bundle": "~5.4.0",
        "symfony/uid": "~5.4.0",
        "symfony/validator": "~5.4.0",
        "symfony/web-link": "~5.4.0",
        "symfony/webpack-encore-bundle": "^1.14",
        "symfony/yaml": "~5.4.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.45",
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.5",
        "symfony/maker-bundle": "^1.38",
        "vincentlanglet/twig-cs-fixer": "^3.5"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "repositories": {
        "itk-dev/getorganized-api-client-php": {
            "type": "vcs",
            "url": "https://github.com/itk-dev/getorganized-api-client-php"
        },
        "kapersoft/sharefile-api": {
            "type": "git",
            "url": "https://github.com/rimi-itk/sharefile-api"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "code-analysis": [
            "@code-analysis/phpstan"
        ],
        "code-analysis/phpstan": [
            "phpstan analyse --level=5 src"
        ],
        "coding-standards-apply/php": [
            "@coding-standards-apply/php-cs-fixer"
        ],
        "coding-standards-apply/php-cs-fixer": [
            "vendor/bin/php-cs-fixer --allow-risky=yes fix"
        ],
        "coding-standards-apply/twig": [
            "@coding-standards-apply/twig-cs-fixer"
        ],
        "coding-standards-apply/twig-cs-fixer": [
            "vendor/bin/twig-cs-fixer fix templates"
        ],
        "coding-standards-check": [
            "@coding-standards-check/php",
            "@coding-standards-check/twig"
        ],
        "coding-standards-check/php": [
            "@coding-standards-check/php-cs-fixer"
        ],
        "coding-standards-check/twig-cs-fixer": [
            "vendor/bin/twig-cs-fixer fix templates"
        ]
    }
}
